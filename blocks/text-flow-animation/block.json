{
  "apiVersion": 2,
  "name": "integlight/text-flow-animation",
  "title": "【Integlight】text flow animation",
  "category": "widgets",
  "icon": "editor-alignleft",
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "content": {
      "type": "string",
      "default": "SAMPLE TEXT"
    },
    "fontSize": {
      "type": "number",
      "default": 60
    },
    "color": {
      "type": "string",
      "default": "#000000"
    },
    "fontFamily": {
      "type": "string",
      "default": "Arial, sans-serif"
    }
  },
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "style": "file:./build/style-index.css"
}