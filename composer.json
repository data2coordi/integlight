{
    "name": "data2coordi/integlight",
    "type": "wordpress-theme",
    "description": "Hi. I'm integlight, a custom starter theme built for creative WordPress development. Use me as a starting point for your next awesome WordPress theme.",
    "keywords": [
        "WordPress",
        "Themes",
        "integlight"
    ],
    "homepage": "https://github.com/data2coordi/integlight",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "data2coordi",
            "homepage": "https://auroralab-design.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "wptrt/wpthemereview": "^0.2.1",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "wp-cli/i18n-command": "^2.2.5",
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^1.1"
    },
    "scripts": {
        "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor .",
        "make-pot": "wp i18n make-pot . languages/integlight.pot"
    },
    "support": {
        "issues": "https://github.com/data2coordi/integlight/issues",
        "source": "https://github.com/data2coordi/integlight"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "process-timeout": 1200
    }
}